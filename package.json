{
  "name": "@gkampitakis/mongo-client",
  "version": "2.0.0",
  "description": "Nodejs wrapper for mongodb",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run clean && npm run lint && jest",
    "playground": "ts-node-dev --respawn --transpileOnly ./playground.ts",
    "playground:mongo": "npm run playground -- server",
    "build": "npm run clean && tsc",
    "format": "prettier --write ./src/**/*",
    "lint": "semistandard | snazzy",
    "clean": "rimraf ./coverage",
    "benchmarks": "npm run build && node ./benchmarks/index.js",
    "release": "semantic-release"
  },
  "keywords": [
    "mongodb",
    "mongo-client",
    "mongo",
    "nodejs mongo client"
  ],
  "author": "Georgios Kampitakis",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.12.6",
    "fast-deep-equal": "^3.1.3",
    "flat": "^5.0.2",
    "kareem": "~2.3.1",
    "mongodb": "^3.6.3",
    "@types/mongodb": "^3.5.33"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "jest": "^26.6.3",
    "mongoose": "^5.10.14",
    "semantic-release": "^17.2.2",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverageFrom": []
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gkampitakis/mongo-client.git"
  }
}
